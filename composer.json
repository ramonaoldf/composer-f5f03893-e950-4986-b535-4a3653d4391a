{
    "name": "laravel/workos",
    "description": "Utilities for integrating the Laravel starter kits with WorkOS.",
    "keywords": ["laravel", "workos"],
    "homepage": "https://github.com/laravel/workos",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/workos/issues",
        "source": "https://github.com/laravel/workos"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "workos/workos-php": "^4.18",
        "firebase/php-jwt": "^6.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\WorkOS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": []
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
